int xc=250;
int siz=500;
int siz1=64;
String dir;
void setup() {
Serial.begin(115200);
IrReceiver.begin(IR_RECEIVE_PIN, ENABLE_LED_FEEDBACK); 
 display.begin(SSD1306_SWITCHCAPVCC, 0x3D);
display.clearDisplay();
delay(1);
display.setCursor(0,0);
display.setTextColor(WHITE);
display.setTextSize(2);
display.println("ESP CRAFT");
display.display();
delay(100);
display.clearDisplay();
}

void loop() {
Serial.println(xc);  
display.drawBitmap(xc, 0, land, siz, siz1, 1);
display.drawBitmap(0, 0, manhands, 128, 64, 1);
display.setCursor(0,0);
display.setTextColor(WHITE);
display.setTextSize(1);
display.println(dir);
display.display();
delay(1);
display.clearDisplay();
ircontrol();
properties();
}

void ircontrol(){
if (IrReceiver.decode()) {
   if(IrReceiver.decodedIRData.command == RIGHT_BUTT1 ){
xc=xc-10; 
  }  
     if(IrReceiver.decodedIRData.command == LEFT_BUTT1 ){
xc=xc+10;
   
  } 
       if(IrReceiver.decodedIRData.command== DOWN_BUTT1 ){

siz1=siz1+1;  
  }
         if(IrReceiver.decodedIRData.command== UP_BUTT1){

siz1=siz1-1;  
  }
}   
    IrReceiver.resume();                             
   delay(100);  
  }

void properties(){
if(xc<=0 && xc>-250){
dir="W";  
  }
if(xc<=-250){
dir="E";  
  }if(xc>0){
xc=0;  
  }  
if(xc<-370){
xc=-370;  
  } 
 
}
  
